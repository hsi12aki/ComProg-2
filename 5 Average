  import java.util.Scanner;
  
  public class Main {
      public static void main(String[] args) {
        
        Scanner my1st = new Scanner(System.in);
        System.out.println("1st number");
        int first = my1st.nextInt();
        
        Scanner my2nd = new Scanner(System.in);
        System.out.println("2nd number");
        int second = my2nd.nextInt();
        
        Scanner my3rd = new Scanner(System.in);
        System.out.println("3rd number");
        int third = my3rd.nextInt();
        
        Scanner my4th = new Scanner(System.in);
        System.out.println("4th number");
        int fourth = my4th.nextInt();
        
        Scanner my5th = new Scanner(System.in);
        System.out.println("5th number");
        int fifth = my5th.nextInt();
        
        int average = ((first+second+third+fourth+fifth)/5);
        System.out.println("Average number: " + average);
        
        if(average >= 51 && average <= 100)
                  System.out.println("Output: ++");
            else if(average == 50)
                  System.out.println("Output: ==");
            else if(average >= 0 && average <= 49)
                  System.out.println("Output: --");
            else
                  System.out.println("Output Average out of Range");
                  
      }
  }
